  "dependencies": {
    "@types/lodash.merge": "^4.6.6",
    "@types/ramda": "^0.27.44",
    "ajv": "^6.12.6",
    "handlebars": "^4.7.7",
    "lodash.merge": "^4.6.2",
    "map-any": "^0.2.1",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@integreat/ts-dev-setup": "^2.0.2",
